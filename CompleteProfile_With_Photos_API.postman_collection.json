								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/complete-profile",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"complete-profile"
							]
						},
						"description": "Get authenticated user's complete profile - now includes profile photo in base64 format"
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Response has profile photo fields', function () {",
									"    const response = pm.response.json();",
									"    pm.expect(response.data).to.have.property('profilePhotoBase64');",
									"    pm.expect(response.data).to.have.property('profilePhotoContentType');",
									"    pm.expect(response.data).to.have.property('hasProfilePhoto');",
									"});",
									"",
									"pm.test('Profile includes contact details (authenticated)', function () {",
									"    const response = pm.response.json();",
									"    // Authenticated users should see contact details",
									"    pm.expect(response.data).to.have.property('contactDetails');",
									"});",
									"",
									"pm.test('Profile photo data is valid if present', function () {",
									"    const response = pm.response.json();",
									"    if (response.data.hasProfilePhoto) {",
									"        pm.expect(response.data.profilePhotoBase64).to.not.be.null;",
									"        pm.expect(response.data.profilePhotoContentType).to.not.be.null;",
									"        console.log('Profile photo found - Content Type:', response.data.profilePhotoContentType);",
									"    } else {",
									"        console.log('No profile photo found for authenticated user');",
									"    }",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				},
